{
    "name": "getsno/relesys-users",
    "description": "Laravel wrapper package for the Relesys User Management API",
    "keywords": [
        "getsno",
        "relesys",
        "relesys-laravel",
        "relesys-api",
        "relesys-users"
    ],
    "homepage": "https://github.com/getsno/relesys-users",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Anton Samofal",
            "email": "devtosha@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Getsno\\Relesys\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Getsno\\Relesys\\Tests\\": "tests"
        },
        "files": [
          "src/FakeResponses.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
              "RelesyServiceProvider"
            ],
            "aliases": {
                "Relesys": "RelesysFacade"
            }
        }
    }
}
